    "homepage": "https://github.com/angusm73/laravel-mail-editor",
    "keywords": ["Laravel", "mailEclipse", "mailable-editor", "laravel-mail", "laravel-mailable", "mailable", "markdown"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "angusm73\\mailEclipse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "angusm73\\mailEclipse\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "angusm73\\mailEclipse\\mailEclipseServiceProvider"
            ],
            "aliases": {
                "mailEclipse": "angusm73\\mailEclipse\\Facades\\mailEclipse"
            }
        }
    }
}
